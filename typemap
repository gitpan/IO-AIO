aio_req		T_AIO
aio_req_ornot	T_AIO_ORNOT
SV8 *		T_SV8

INPUT

T_AIO
	if (!($var = SvAIO_REQ ($arg)))
          croak (\"busy \" AIO_REQ_KLASS \" object expected\")

T_AIO_ORNOT
	if (!($var = SvAIO_REQ ($arg)))
	  XSRETURN_EMPTY

T_SV8
	($var) = $arg;
        if (SvPOKp ($var) && !sv_utf8_downgrade ($var, 1))
          croak (\"\\\"%s\\\" argument must be byte/octet-encoded\", \"$var\")
